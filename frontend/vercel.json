{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/(.*)\\.(css|js|png|jpg|jpeg|svg|ico|txt|map|webp)", "destination": "/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
